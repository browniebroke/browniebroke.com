  "license": "MIT",
  "repository": "https://github.com/browniebroke/browniebroke.com.git",
  "homepage": "https://browniebroke.com",
  "dependencies": {
    "@browniebroke/react-ui-components": "2.1.0",
    "@fontsource/cabin-condensed": "4.5.9",
    "@fontsource/patua-one": "4.5.9",
    "@mdx-js/react": "2.2.1",
    "babel-plugin-styled-components": "2.0.7",
    "gatsby": "5.3.2",
    "gatsby-plugin-csp": "1.1.4",
    "gatsby-plugin-feed": "5.3.1",
    "gatsby-plugin-google-gtag": "5.3.0",
    "gatsby-plugin-image": "3.3.2",
    "gatsby-plugin-manifest": "5.3.1",
    "gatsby-plugin-mdx": "5.3.1",
    "gatsby-plugin-netlify": "5.1.0",
    "gatsby-plugin-offline": "6.3.1",
    "gatsby-plugin-react-helmet": "6.3.0",
    "gatsby-plugin-robots-txt": "1.8.0",
    "gatsby-plugin-sharp": "5.3.2",
    "gatsby-plugin-sitemap": "6.3.1",
    "gatsby-plugin-styled-components": "6.3.0",
    "gatsby-plugin-typescript": "5.3.1",
    "gatsby-remark-images": "7.3.1",
    "gatsby-remark-smartypants": "6.3.0",
    "gatsby-remark-vscode": "3.3.1",
    "gatsby-source-filesystem": "5.3.1",
    "gatsby-transformer-sharp": "5.3.1",
    "htmlparser2": "8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-icons": "4.7.1",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "6.1.1",
    "rehype-external-links": "2.0.1",
    "rehype-slug": "5.1.0",
    "replace-in-file": "6.3.5",
    "sanitize.css": "13.0.0",
    "slugify": "1.6.5",
    "styled-components": "5.3.6",
    "toml": "github:GalAster/vscode-toml",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/react-helmet": "6.1.6",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/parser": "5.46.1",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "prettier": "2.8.1"
  },
  "scripts": {
    "build": "gatsby build && node ./csp-util.js",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc",
    "format": "prettier --write ."
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "prettier"
    ],
    "plugins": [
      "react",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "semi": false,
          "singleQuote": true,
          "trailingComma": "es5"
        }
      ]
    }
  },
  "renovate": {
    "extends": [
      "github>browniebroke/renovate-configs:js-app"
    ]
  }
}
