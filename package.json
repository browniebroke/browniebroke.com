  "license": "MIT",
  "repository": "https://github.com/browniebroke/browniebroke.com.git",
  "homepage": "https://browniebroke.com",
  "dependencies": {
    "@browniebroke/react-ui-components": "1.8.0",
    "@fontsource/cabin-condensed": "4.4.5",
    "@fontsource/patua-one": "4.4.5",
    "babel-plugin-styled-components": "1.13.2",
    "gatsby": "3.8.1",
    "gatsby-link": "3.8.0",
    "gatsby-plugin-csp": "1.1.3",
    "gatsby-plugin-feed": "3.8.0",
    "gatsby-plugin-google-gtag": "3.8.0",
    "gatsby-plugin-image": "1.8.0",
    "gatsby-plugin-manifest": "3.8.0",
    "gatsby-plugin-netlify": "3.8.0",
    "gatsby-plugin-offline": "4.8.0",
    "gatsby-plugin-react-helmet": "4.8.0",
    "gatsby-plugin-robots-txt": "1.6.8",
    "gatsby-plugin-sharp": "3.8.0",
    "gatsby-plugin-sitemap": "4.4.0",
    "gatsby-plugin-styled-components": "4.9.0",
    "gatsby-plugin-typescript": "3.8.0",
    "gatsby-plugin-typography": "3.8.0",
    "gatsby-remark-images": "5.5.0",
    "gatsby-remark-smartypants": "4.5.0",
    "gatsby-remark-vscode": "3.3.0",
    "gatsby-source-filesystem": "3.8.0",
    "gatsby-transformer-remark": "4.5.0",
    "gatsby-transformer-sharp": "3.8.0",
    "htmlparser2": "6.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-icons": "4.2.0",
    "react-typography": "0.16.20",
    "remark-external-links": "8.0.0",
    "replace-in-file": "6.2.0",
    "slugify": "1.5.3",
    "styled-components": "5.3.0",
    "toml": "github:GalAster/vscode-toml",
    "typescript": "4.3.5",
    "typography": "0.16.21",
    "typography-theme-funston": "0.16.19"
  },
  "devDependencies": {
    "@types/node": "14.17.5",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.9",
    "@types/react-helmet": "6.1.2",
    "@types/styled-components": "5.1.11",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "prettier": "2.3.2"
  },
  "scripts": {
    "build": "gatsby build && node ./csp-util.js",
    "develop": "gatsby develop",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc",
    "format": "prettier --write ."
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "prettier"
    ],
    "plugins": [
      "react",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "semi": false,
          "singleQuote": true,
          "trailingComma": "es5"
        }
      ]
    }
  },
  "renovate": {
    "extends": [
      "github>browniebroke/renovate-configs:js-app"
    ]
  }
}
