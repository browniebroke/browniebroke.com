{
  "name": "blog-perso",
  "version": "1.0.0",
  "description": "Code for my personal Blog",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/browniebroke/browniebroke.com.git",
  "homepage": "https://browniebroke.com",
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "dependencies": {
    "@browniebroke/react-ui-components": "1.8.3",
    "@fontsource/cabin-condensed": "4.5.7",
    "@fontsource/patua-one": "4.5.5",
    "babel-plugin-styled-components": "2.0.7",
    "gatsby": "4.14.0",
    "gatsby-plugin-csp": "1.1.3",
    "gatsby-plugin-feed": "4.13.0",
    "gatsby-plugin-google-gtag": "4.13.0",
    "gatsby-plugin-image": "2.13.0",
    "gatsby-plugin-manifest": "4.13.0",
    "gatsby-plugin-netlify": "4.3.1",
    "gatsby-plugin-offline": "5.13.0",
    "gatsby-plugin-react-helmet": "5.13.0",
    "gatsby-plugin-robots-txt": "1.7.1",
    "gatsby-plugin-sharp": "4.13.0",
    "gatsby-plugin-sitemap": "5.13.0",
    "gatsby-plugin-styled-components": "5.14.0",
    "gatsby-plugin-typescript": "4.13.0",
    "gatsby-plugin-typography": "4.13.0",
    "gatsby-remark-images": "6.13.0",
    "gatsby-remark-smartypants": "5.13.0",
    "gatsby-remark-vscode": "3.3.1",
    "gatsby-source-filesystem": "4.13.0",
    "gatsby-transformer-remark": "5.13.0",
    "gatsby-transformer-sharp": "4.13.0",
    "htmlparser2": "8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-icons": "4.3.1",
    "react-typography": "0.16.20",
    "remark-external-links": "8.0.0",
    "replace-in-file": "6.3.2",
    "slugify": "1.6.5",
    "styled-components": "5.3.5",
    "toml": "github:GalAster/vscode-toml",
    "typescript": "4.6.4",
    "typography": "0.16.21",
    "typography-theme-funston": "0.16.19"
  },
  "devDependencies": {
    "@types/node": "16.11.34",
    "@types/react": "17.0.45",
    "@types/react-dom": "17.0.16",
    "@types/react-helmet": "6.1.5",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/parser": "5.23.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "prettier": "2.6.2"
  },
  "scripts": {
    "build": "gatsby build && node ./csp-util.js",
    "develop": "gatsby develop",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc",
    "format": "prettier --write ."
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "prettier"
    ],
    "plugins": [
      "react",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "semi": false,
          "singleQuote": true,
          "trailingComma": "es5"
        }
      ]
    }
  },
  "renovate": {
    "extends": [
      "github>browniebroke/renovate-configs:js-app"
    ]
  }
}
