  "license": "MIT",
  "repository": "https://github.com/browniebroke/browniebroke.com.git",
  "homepage": "https://browniebroke.com",
  "dependencies": {
    "@chakra-ui/gatsby-plugin": "3.1.3",
    "@chakra-ui/react": "2.8.2",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@fontsource/cabin-condensed": "5.0.20",
    "@fontsource/patua-one": "5.0.19",
    "framer-motion": "11.1.9",
    "gatsby": "5.13.4",
    "gatsby-plugin-csp": "1.1.4",
    "gatsby-plugin-feed": "5.13.1",
    "gatsby-plugin-google-gtag": "5.13.1",
    "gatsby-plugin-image": "3.13.1",
    "gatsby-plugin-manifest": "5.13.1",
    "gatsby-plugin-mdx": "5.13.1",
    "gatsby-plugin-netlify": "5.1.1",
    "gatsby-plugin-offline": "6.13.2",
    "gatsby-plugin-react-helmet": "6.13.1",
    "gatsby-plugin-robots-txt": "1.8.0",
    "gatsby-plugin-sharp": "5.13.1",
    "gatsby-plugin-sitemap": "6.13.1",
    "gatsby-plugin-typescript": "5.13.1",
    "gatsby-remark-images": "7.13.1",
    "gatsby-remark-prismjs": "7.13.1",
    "gatsby-remark-smartypants": "6.13.1",
    "gatsby-source-filesystem": "5.13.1",
    "gatsby-transformer-sharp": "5.13.1",
    "htmlparser2": "9.1.0",
    "prismjs": "1.29.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-helmet": "6.1.0",
    "react-icons": "5.2.1",
    "reading-time": "1.5.0",
    "remark-external-links": "8.0.0",
    "replace-in-file": "7.1.0",
    "slugify": "1.6.6",
    "typescript": "5.4.5"
  },
  "devDependencies": {
    "@types/node": "20.12.12",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "@types/react-helmet": "6.1.11",
    "@typescript-eslint/parser": "7.9.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "prettier": "3.2.5"
  },
  "scripts": {
    "build": "gatsby build && node ./csp-util.js",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc",
    "format": "prettier --write ."
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "prettier"
    ],
    "plugins": [
      "react",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "semi": false,
          "singleQuote": true,
          "trailingComma": "es5"
        }
      ]
    }
  },
  "renovate": {
    "extends": [
      "github>browniebroke/renovate-configs:js-app"
    ]
  }
}
